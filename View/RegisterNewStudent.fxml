<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="646.0" prefWidth="508.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ijse.com.student.Controller.RegisterNewStudentController">
   <children>
      <Label layoutX="141.0" layoutY="14.0" text="Register New Student">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label layoutX="34.0" layoutY="70.0" text="Student ID :" />
      <TextField fx:id="txtStudentID" layoutX="129.0" layoutY="66.0" />
      <Label layoutX="34.0" layoutY="108.0" text="Student Name :" />
      <TextField fx:id="txtStudentName" layoutX="129.0" layoutY="104.0" prefHeight="25.0" prefWidth="343.0" />
      <Label layoutX="34.0" layoutY="149.0" text="Address :" />
      <TextField fx:id="txtStudentAddress" layoutX="129.0" layoutY="145.0" prefHeight="25.0" prefWidth="343.0" />
      <Label layoutX="34.0" layoutY="197.0" text="Telephone :" />
      <TextField fx:id="txtTele" layoutX="129.0" layoutY="193.0" promptText="Home" />
      <TextField fx:id="txtMobile" layoutX="323.0" layoutY="193.0" promptText="Mobile" />
      <Label layoutX="34.0" layoutY="244.0" text="Email :" />
      <TextField fx:id="txtEmail" layoutX="129.0" layoutY="240.0" prefHeight="25.0" prefWidth="343.0" promptText="Example : example@gmail.com" />
      <Label layoutX="34.0" layoutY="296.0" text="Date of Birth :" />
      <Label layoutX="34.0" layoutY="342.0" text="Gender :" />
      <Label layoutX="34.0" layoutY="384.0" text="School :" />
      <TextField fx:id="txtSchool" layoutX="130.0" layoutY="380.0" prefHeight="25.0" prefWidth="343.0" />
      <Label layoutX="34.0" layoutY="428.0" text="University :" />
      <TextField fx:id="txtUniversity" layoutX="130.0" layoutY="424.0" prefHeight="25.0" prefWidth="343.0" />
      <TextField fx:id="txtFaculty" layoutX="130.0" layoutY="472.0" prefHeight="25.0" prefWidth="343.0" />
      <Label layoutX="34.0" layoutY="476.0" text="Faculty :" />
      <Label layoutX="31.0" layoutY="521.0" text="Higher E. Qua :" />
      <Button fx:id="btnSave" layoutX="28.0" layoutY="601.0" mnemonicParsing="false" onAction="#clickNext" prefHeight="25.0" prefWidth="83.0" text="Next" />
      <Button layoutX="390.0" layoutY="601.0" mnemonicParsing="false" onAction="#clickBack" prefHeight="25.0" prefWidth="83.0" text="Back" />
      <TextField fx:id="txtDob" layoutX="130.0" layoutY="292.0" prefHeight="25.0" prefWidth="160.0" />
      <RadioButton fx:id="rdoMale" layoutX="130.0" layoutY="342.0" mnemonicParsing="false" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="gender" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="rdoFemale" layoutX="211.0" layoutY="342.0" mnemonicParsing="false" text="Female" toggleGroup="$gender" />
      <RadioButton fx:id="rdMaster" layoutX="129.0" layoutY="521.0" mnemonicParsing="false" text="Master">
         <toggleGroup>
            <ToggleGroup fx:id="edu" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rdDegree" layoutX="235.0" layoutY="521.0" mnemonicParsing="false" text="Degree" toggleGroup="$edu" />
      <RadioButton fx:id="rdDiploma" layoutX="347.0" layoutY="521.0" mnemonicParsing="false" text="Diploma" toggleGroup="$edu" />
      <RadioButton fx:id="rdAL" layoutX="129.0" layoutY="560.0" mnemonicParsing="false" text="A/L" toggleGroup="$edu" />
      <RadioButton fx:id="rdOL" layoutX="235.0" layoutY="560.0" mnemonicParsing="false" text="O/L" toggleGroup="$edu" />
      <RadioButton fx:id="rdOther" layoutX="347.0" layoutY="560.0" mnemonicParsing="false" text="Other" toggleGroup="$edu" />
   </children>
</AnchorPane>
