<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="470.0" prefWidth="506.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ijse.com.student.Controller.SelectCourseController">
   <children>
      <Label layoutX="172.0" layoutY="15.0" text="Select Courses">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="txtSearchID" layoutX="240.0" layoutY="62.0" onAction="#clickSearchID" prefHeight="25.0" prefWidth="247.0" promptText="Search Student ID" />
      <Label layoutX="185.0" layoutY="149.0" text="Select Course and Batch">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <ComboBox fx:id="comboBatches" layoutX="56.0" layoutY="178.0" prefHeight="25.0" prefWidth="412.0" />
      <Button layoutX="54.0" layoutY="431.0" mnemonicParsing="false" onAction="#clickDelete" prefHeight="25.0" prefWidth="67.0" text="Delete" />
      <TableView fx:id="tblCourse" layoutX="54.0" layoutY="262.0" prefHeight="148.0" prefWidth="427.0">
        <columns>
          <TableColumn prefWidth="426.0" text="Batch" />
        </columns>
      </TableView>
      <Button layoutX="400.0" layoutY="431.0" mnemonicParsing="false" onAction="#clickSave" prefHeight="25.0" prefWidth="81.0" text="Save" />
      <Button layoutX="441.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickBack" text="Back" />
      <Label layoutX="58.0" layoutY="110.0" text="Student ID :" />
      <TextField fx:id="txtStudentID" layoutX="131.0" layoutY="106.0" />
   </children>
</AnchorPane>
